{
  "name": "universal-react-template",
  "version": "0.1.0",
  "description": "webpack 4 SSR react template",
  "main": "index.js",
  "scripts": {
    "start": "node bin/server.prod.js",
    "prod": "npm run build && node bin/server.prod.js",
    "dev": "node bin/server.dev.js",
    "prebuild": "rimraf build",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --progress -p --config webpack/client.prod.js",
    "build:server": "webpack --progress --config webpack/server.prod.js",
    "clean": "rimraf buildClient buildServer",
    "format": "prettier --write \"src/**/*.js\"",
    "eslint": "eslint -c .eslintrc src",
    "lint": "npm run eslint",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:status": "flow status",
    "flow:coverage": "flow coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjlaubscher/universal-react-template.git"
  },
  "keywords": [
    "webpack",
    "node",
    "react",
    "emotion"
  ],
  "author": "Francois Laubscher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjlaubscher/universal-react-template/issues"
  },
  "homepage": "https://github.com/fjlaubscher/universal-react-template#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-universal-import": "^2.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "compression": "^1.7.2",
    "dotenv-webpack": "^1.5.5",
    "emotion": "^9.1.3",
    "emotion-server": "^9.1.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.73.0",
    "prettier": "^1.12.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-emotion": "^9.1.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-universal-component": "^2.9.0",
    "rimraf": "^2.6.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-flush-chunks": "^1.2.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "dependencies": {
    "express": "^4.16.3"
  }
}
